                "rds:Download*",
                "rds:StartDBCluster",
                "rds:CreateDBSnapshot",
                "rds:ListTagsForResource",
                "rds:StopDBCluster",
                "rds:StopDBInstance",
                "rds:CreateDBClusterSnapshot",
                "s3:Get*",
                "s3:List*",
                "s3:HeadBucket",
                "ssm:*",
                "trustedadvisor:*",
                "workspaces:*"
            ],
            "Resource": "*"
        }
    ]
}
