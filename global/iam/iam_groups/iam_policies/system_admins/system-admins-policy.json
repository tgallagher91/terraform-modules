{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "access-analyzer:*",
                "appstream:*",
                "cloudtrail:LookupEvents",
                "cloudtrail:List*",
                "cloudtrail:Describe*",
                "cloudtrail:Get*",
                "cloudwatch:Describe*",
                "cloudwatch:Get*",
                "cloudwatch:List*",
                "ec2:Describe*",
                "ec2:Get*",
                "ec2:ModifyTrafficMirrorSession",
                "ec2:SearchTransitGatewayRoutes",
                "ec2:CreateTags",
                "ec2:CreateSnapshots",
                "ec2:ExportClientVpnClientCertificateRevocationList",
                "ec2:UnmonitorInstances",
                "ec2:MonitorInstances",
                "ec2:ModifyVolume",
                "ec2:DetachNetworkInterface",
                "ec2:ReportInstanceStatus",
                "ec2:StopInstances",
                "ec2:CreateVolume",
                "ec2:CreateSnapshot",
                "ec2:ExportClientVpnClientConfiguration",
                "ec2:StartInstances",
                "ec2:AttachVolume",
                "ec2:RebootInstances",
                "ec2:CopySnapshot",
                "ec2:DetachVolume",
